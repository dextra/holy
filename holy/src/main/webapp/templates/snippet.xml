<engine>
	<action selector="#content">
		<div class="togglePanel">
			<h3>#header</h3>
			<div>
				<pre class="snippet">
				</pre>
			</div>
		</div>
	</action>
	<script>
		<![CDATA[
			$('#content .snippet').text($('#header').html());
			$('#content .snippet').snippet('html', {style:"acid",showNum:false});
		]]>
	</script>
</engine>
