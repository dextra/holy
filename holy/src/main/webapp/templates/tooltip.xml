<engine>
	<action selector="#content">
		<div class="tabPanel">
			<ul>
				<li>
					<a>Demo</a>
				</li>
				<li>
					<a>Documentation</a>
				</li>
				<li>
					<a>Code</a>
				</li>
			</ul>
			<div class="demo">
				<a href="#tooltip" title="Tooltip demo">Hover me!</a>
			</div>
			<div class="doc">See documentation on <a href="http://onehackoranother.com/projects/jquery/tipsy/" target="_blank">http://onehackoranother.com/projects/jquery/tipsy/</a></div>
			<div class="code"><pre></pre></div>
		</div>
	</action>
	<script>
		<![CDATA[
		$('.demo a').tipsy();
		$('.tabPanel').tabPanel();
		$('.code pre').text($('.demo').html());
		$('.code pre').snippet('html', {style:"ide-eclipse"});
		]]>
	</script>
</engine>
