<engine>
	<action selector="#content">
		<h3>
			Tool<span>tip</span>
		</h3>
		<div class="tabPanel">
			<ul>
				<li>
					<a href="#!">Demo</a>
				</li>
				<li>
					<a href="#!">Documentation</a>
				</li>
				<li>
					<a href="#!">Script</a>
				</li>
				<li>
					<a href="#!">HTML</a>
				</li>
				<li>
					<a href="#!">Download</a>
				</li>
			</ul>
			<div class="demo">
				<a href="#tooltip" title="Tooltip demo">Hover me!</a>
			</div>
			<div class="doc">
				See documentation on
				<a target="_blank" href="http://onehackoranother.com/projects/jquery/tipsy/">http://onehackoranother.com/projects/jquery/tipsy/</a>
			</div>
			<div class="script">
				<pre>$('.demo a').tipsy();</pre>
			</div>
			<div class="code">
				<pre></pre>
			</div>
			<div class="download">
				<div class="buttons">
					<a class="btDownload" target="_blank" title="Click here to download JavaScript file." href="holy/third/tooltip.tipsy/jquery.tipsy.js">jquery.tipsy.js</a>
					<a class="btDownload" target="_blank" title="Click here to download CSS file." href="holy/third/tooltip.tipsy/jquery.tipsy.css">jquery.tipsy.css</a>
				</div>
			</div>
		</div>
	</action>
	<script>
		<![CDATA[
		$('.demo a').tipsy();
		$('.tabPanel').tabPanel();
		$('.code pre').text($('.demo').html());
		$('.code pre').snippet('html', {style:"ide-eclipse",showNum:false});
		]]>
	</script>
</engine>
